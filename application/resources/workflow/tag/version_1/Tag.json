{
    "version": "1.0",
    "name": "Tag Workflow",
    "desc": "Description of the workflow for Tag",
    "initialState": "initial_state",
    "active": true,
    "states": {
        "initial_state": {
            "transitions": [
                {
                    "name": "transition_to_active",
                    "next": "active",
                    "manual": false,
                    "processors": [
                        {
                            "name": "TagCreationProcessor",
                            "executionMode": "SYNC",
                            "config": {
                                "calculationNodesTags": "cyoda_application"
                            }
                        }
                    ]
                }
            ]
        },
        "active": {
            "transitions": [
                {
                    "name": "transition_to_inactive",
                    "next": "inactive",
                    "manual": true
                }
            ]
        },
        "inactive": {
            "transitions": [
                {
                    "name": "transition_to_active",
                    "next": "active",
                    "manual": true
                },
                {
                    "name": "transition_to_archived",
                    "next": "archived",
                    "manual": true,
                    "processors": [
                        {
                            "name": "TagArchiveProcessor",
                            "executionMode": "SYNC",
                            "config": {
                                "calculationNodesTags": "cyoda_application"
                            }
                        }
                    ]
                }
            ]
        }
    }
}