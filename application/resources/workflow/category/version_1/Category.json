{
    "version": "1.0",
    "name": "Category Workflow",
    "desc": "Description of the workflow for Category",
    "initialState": "initial_state",
    "active": true,
    "states": {
        "initial_state": {
            "transitions": [
                {
                    "name": "transition_to_active",
                    "next": "active",
                    "manual": false,
                    "processors": [
                        {
                            "name": "CategoryCreationProcessor",
                            "executionMode": "SYNC",
                            "config": {
                                "calculationNodesTags": "cyoda_application"
                            }
                        }
                    ]
                }
            ]
        },
        "active": {
            "transitions": [
                {
                    "name": "transition_to_inactive",
                    "next": "inactive",
                    "manual": true
                }
            ]
        },
        "inactive": {
            "transitions": [
                {
                    "name": "transition_to_active",
                    "next": "active",
                    "manual": true,
                    "processors": [
                        {
                            "name": "CategoryReactivationProcessor",
                            "executionMode": "SYNC",
                            "config": {
                                "calculationNodesTags": "cyoda_application"
                            }
                        }
                    ]
                },
                {
                    "name": "transition_to_archived",
                    "next": "archived",
                    "manual": true,
                    "processors": [
                        {
                            "name": "CategoryArchiveProcessor",
                            "executionMode": "SYNC",
                            "config": {
                                "calculationNodesTags": "cyoda_application"
                            }
                        }
                    ],
                    "criterion": {
                        "type": "function",
                        "function": {
                            "name": "CategoryEmptyCriterion",
                            "config": {
                                "calculationNodesTags": "cyoda_application"
                            }
                        }
                    }
                }
            ]
        }
    },
    "archived": {
        "transitions": []
    }
}