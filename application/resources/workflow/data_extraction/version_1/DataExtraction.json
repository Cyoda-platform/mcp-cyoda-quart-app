{
  "version": "1.0",
  "name": "DataExtraction Workflow",
  "desc": "Workflow for DataExtraction entity - handles scheduled API data extraction and processing",
  "initialState": "initial_state",
  "active": true,
  "states": {
    "initial_state": {
      "transitions": [
        {
          "name": "schedule",
          "next": "scheduled",
          "manual": false
        }
      ]
    },
    "scheduled": {
      "transitions": [
        {
          "name": "validate",
          "next": "validated",
          "manual": false,
          "criterion": {
            "type": "function",
            "function": {
              "name": "DataExtractionValidationCriterion",
              "config": {
                "calculationNodesTags": "cyoda_application"
              }
            }
          }
        }
      ]
    },
    "validated": {
      "transitions": [
        {
          "name": "extract",
          "next": "extracting",
          "manual": false,
          "processors": [
            {
              "name": "DataExtractionProcessor",
              "executionMode": "SYNC",
              "config": {
                "calculationNodesTags": "cyoda_application"
              }
            }
          ]
        }
      ]
    },
    "extracting": {
      "transitions": [
        {
          "name": "process",
          "next": "processed",
          "manual": false,
          "processors": [
            {
              "name": "DataProcessingProcessor",
              "executionMode": "SYNC",
              "config": {
                "calculationNodesTags": "cyoda_application"
              }
            }
          ]
        }
      ]
    },
    "processed": {
      "transitions": [
        {
          "name": "complete",
          "next": "completed",
          "manual": false
        }
      ]
    },
    "completed": {
      "transitions": []
    }
  }
}
