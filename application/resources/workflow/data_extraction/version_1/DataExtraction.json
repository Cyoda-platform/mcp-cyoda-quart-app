{
  "version": "1.0",
  "name": "DataExtraction Workflow",
  "desc": "Workflow for DataExtraction entity handling API calls and data processing",
  "initialState": "initial_state",
  "active": true,
  "states": {
    "initial_state": {
      "transitions": [
        {
          "name": "schedule",
          "next": "scheduled",
          "manual": false
        }
      ]
    },
    "scheduled": {
      "transitions": [
        {
          "name": "execute",
          "next": "executing",
          "manual": true,
          "processors": [
            {
              "name": "DataExtractionProcessor",
              "executionMode": "SYNC",
              "config": {
                "calculationNodesTags": "cyoda_application"
              }
            }
          ]
        }
      ]
    },
    "executing": {
      "transitions": [
        {
          "name": "validate_extraction",
          "next": "validated",
          "manual": false,
          "criterion": {
            "type": "function",
            "function": {
              "name": "DataExtractionValidationCriterion",
              "config": {
                "calculationNodesTags": "cyoda_application"
              }
            }
          }
        }
      ]
    },
    "validated": {
      "transitions": [
        {
          "name": "complete",
          "next": "completed",
          "manual": false
        }
      ]
    },
    "completed": {
      "transitions": []
    }
  }
}
