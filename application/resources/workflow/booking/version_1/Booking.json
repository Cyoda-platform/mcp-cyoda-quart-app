{
  "version": "1.0",
  "name": "Booking Workflow",
  "desc": "Workflow for processing booking data from Restful Booker API",
  "initialState": "initial_state",
  "active": true,
  "states": {
    "initial_state": {
      "transitions": [
        {
          "name": "create",
          "next": "created",
          "manual": false
        }
      ]
    },
    "created": {
      "transitions": [
        {
          "name": "validate",
          "next": "validated",
          "manual": false,
          "criterion": {
            "type": "function",
            "function": {
              "name": "BookingValidationCriterion",
              "config": {
                "calculationNodesTags": "cyoda_application"
              }
            }
          }
        }
      ]
    },
    "validated": {
      "transitions": [
        {
          "name": "retrieve_data",
          "next": "data_retrieved",
          "manual": false,
          "processors": [
            {
              "name": "BookingDataRetrievalProcessor",
              "executionMode": "SYNC",
              "config": {
                "calculationNodesTags": "cyoda_application"
              }
            }
          ]
        }
      ]
    },
    "data_retrieved": {
      "transitions": [
        {
          "name": "process",
          "next": "processed",
          "manual": false,
          "processors": [
            {
              "name": "BookingDataProcessor",
              "executionMode": "SYNC",
              "config": {
                "calculationNodesTags": "cyoda_application"
              }
            }
          ]
        }
      ]
    },
    "processed": {
      "transitions": [
        {
          "name": "complete",
          "next": "completed",
          "manual": false
        }
      ]
    },
    "completed": {
      "transitions": []
    }
  }
}
