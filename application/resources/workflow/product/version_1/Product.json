[
    {
        "version": "1.0",
        "name": "Product Workflow",
        "desc": "Workflow for Product performance analysis from Pet Store API data",
        "initialState": "initial_state",
        "active": true,
        "states": {
            "initial_state": {
                "transitions": [
                    {
                        "name": "extract",
                        "next": "extracted",
                        "manual": false
                    }
                ]
            },
            "extracted": {
                "transitions": [
                    {
                        "name": "validate",
                        "next": "validated",
                        "manual": false,
                        "criterion": {
                            "type": "function",
                            "function": {
                                "name": "ProductValidationCriterion",
                                "config": {
                                    "calculationNodesTags": "cyoda_application"
                                }
                            }
                        }
                    }
                ]
            },
            "validated": {
                "transitions": [
                    {
                        "name": "analyze",
                        "next": "analyzed",
                        "manual": false,
                        "processors": [
                            {
                                "name": "ProductAnalysisProcessor",
                                "executionMode": "SYNC",
                                "config": {
                                    "calculationNodesTags": "cyoda_application"
                                }
                            }
                        ]
                    }
                ]
            },
            "analyzed": {
                "transitions": [
                    {
                        "name": "complete",
                        "next": "completed",
                        "manual": false
                    }
                ]
            },
            "completed": {
                "transitions": []
            }
        }
    }
]
