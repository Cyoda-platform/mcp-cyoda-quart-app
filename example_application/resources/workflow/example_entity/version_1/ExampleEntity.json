[
    {
        "version": "1.0",
        "name": "ExampleEntity Workflow",
        "desc": "Description of the workflow for ExampleEntity",
        "initialState": "initial_state",
        "active": true,
        "states": {
            "initial_state": {
                "transitions": [
                    {
                        "name": "create",
                        "next": "created",
                        "manual": false
                    }
                ]
            },
            "created": {
                "transitions": [
                    {
                        "name": "validate",
                        "next": "validated",
                        "manual": false,
                        "criterion": {
                            "type": "function",
                            "function": {
                                "name": "ExampleEntityValidationCriterion",
                                "config": {
                                    "calculationNodesTags": "cyoda_application"
                                }
                            }
                        }
                    }
                ]
            },
            "validated": {
                "transitions": [
                    {
                        "name": "process",
                        "next": "processed",
                        "manual": false,
                        "processors": [
                            {
                                "name": "ExampleEntityProcessor",
                                "executionMode": "SYNC",
                                "config": {
                                    "calculationNodesTags": "cyoda_application"
                                }
                            }
                        ]
                    }
                ]
            },
            "processed": {
                "transitions": [
                    {
                        "name": "complete",
                        "next": "completed",
                        "manual": false
                    }
                ]
            },
            "completed": {
                "transitions": []
            }
        }
    }
]
